package fila;

public class FilaInt {
	public final int N = 8;
	int dados[] = new int[N];
	int ini, fim;

	public void Init() {
		ini = 0;
		fim = 0;
	}

	public boolean IsEmpty() {
		if (ini == 0 && fim == 0)
			return true;
		else
			return false;
	}

	public boolean IsFull() {
		if (fim == N)
			return true;
		else
			return false;
	}

	public void Enqueue() {
		if (!IsFull()) {

		}
	}

	public void Dequeue() {
		if (!IsEmpty()) {
		}
	}

	public void Clean() {
		if (!IsEmpty()) {
			ini = 0;
			fim = 0;
		}
	}

}
